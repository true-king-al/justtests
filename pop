var win = window.open('https://media.tenor.com/W9t4G8la9w4AAAAd/you-are-idiot.gif', 'null','width=640,height=468,status=0,toolbar=0'); 
var timer = setInterval(function() { 
    if(win.closed) {
        clearInterval(timer);
        var win = window.open('https://media.tenor.com/W9t4G8la9w4AAAAd/you-are-idiot.gif', 'null','width=640,height=468,status=0,toolbar=0'); 
    }
}, 1000);
